{
    "name": "pianissimo",
    "productName": "pianissimo",
    "version": "0.2.834",
    "description": "pianissimo",
    "main": ".webpack/main",
    "scripts": {
        "build": "npm run version-patch && npm-run-all lint package",
        "build:webpack": "webpack --progress --config webpack.main.config.ts",
        "eslint": "eslint --fix -c .eslintrc.js --ext .ts .",
        "lint": "npm run eslint",
        "start": "npm run version-patch && npm run lint && electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "postinstall": "electron-rebuild",
        "version-patch": "npx auto-version --patch",
        "version-minor": "npx auto-version --minor",
        "version-major": "npx auto-version --major"
    },
    "keywords": [],
    "author": {
        "name": "ammatwain",
        "email": "amedeo@sorpreso.it"
    },
    "license": "MIT",
    "devDependencies": {
        "@electron-forge/cli": "^6.2.1",
        "@electron-forge/maker-deb": "^6.2.1",
        "@electron-forge/maker-rpm": "^6.2.1",
        "@electron-forge/maker-squirrel": "^6.2.1",
        "@electron-forge/maker-zip": "^6.2.1",
        "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
        "@electron-forge/plugin-webpack": "^6.2.1",
        "@electron/rebuild": "^3.2.13",
        "@types/better-sqlite3": "^7.6.4",
        "@types/xmldoc": "^1.1.6",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "@vercel/webpack-asset-relocator-loader": "^1.7.3",
        "auto-version-js": "^0.3.10",
        "css-loader": "^6.8.1",
        "css-modules-typescript-loader": "^4.0.1",
        "electron": "^25.8.0",
        "eslint": "^8.46.0",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-jsdoc": "^46.5.0",
        "eslint-plugin-no-null": "^1.0.2",
        "filemanager-webpack-plugin": "^8.0.0",
        "fork-ts-checker-webpack-plugin": "^7.3.0",
        "node-loader": "^2.0.0",
        "sass": "^1.64.2",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "typescript": "~4.5.4",
        "webpack-cli": "^5.1.4"
    },
